<h1><%= @category.name %> DIY projects</h1>

<%= link_to "(+) Add New Project", new_category_project_path(@category) %>


<% @category.projects.each do |project| %>
  <p><%= link_to project.name, category_project_path(@category, project) %>  <%= link_to "(edit)", edit_category_project_path(@category, project) %></p>
<% end %>
